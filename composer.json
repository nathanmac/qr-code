{
  "name": "nathanmac/qr-code",
  "description": "Laravel package to providing a simple QR Code generator route",
  "keywords": [
    "qr",
    "qrcode",
    "route",
    "generator"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nathan Macnamara",
      "email": "hola@nathanmac.com"
    }
  ],
  "require": {
    "endroid/qrcode": "^1.5",
    "illuminate/routing": "^5.0",
    "illuminate/support": "^5.0"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "scrutinizer/ocular": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Nathanmac\\Utilities\\QRCode\\": "src/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
